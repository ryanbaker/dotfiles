          tegra_sinterface = "serial_a";         //STR(TEGRA_SINTERFACE);
          discontinuous_clk = "no";
          dpcm_enable = "false";
          cil_settletime = "0";
          csi_pixel_bit_depth = "10";
          mode_type = "bayer";
          pixel_phase = "rggb";
